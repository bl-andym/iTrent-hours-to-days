{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "32": "images/icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://ce0752li.webitrent.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "32": "images/icon.png"
  }
}